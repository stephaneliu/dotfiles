lolcommits_home=$HOME/Documents/Resource/lolcommits
alias lol='LOLCOMMITS_DIR=$lolcommits_home lolcommits'
alias lolc='LOLCOMMITS_DIR=$lolcommits_home lolcommits --capture --fork --delay "$LOL_DELAY" --device "$WEBCAM"'
alias loll='LOLCOMMITS_DIR=$lolcommits_home lolcommits --last'
alias lolt='LOLCOMMITS_DIR=$lolcommits_home lolcommits --timelapse'
