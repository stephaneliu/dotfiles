zshrc="$HOME/.zshrc"

if [ -L $zshrc ]; then
  echo "+++ rcm installed correctly."

  echo "*** home dir"
  ls -alh ~/
  echo "*** zshrc contents"
  cat ~/.zshrc
else
  echo "*** rcm did not install dotfiles correctly"
  echo "*** $HOME"
  ls -alh ~/
  echo "Current dir $PWD"
  echo $PWD
  echo "ls $PWD"
  ls -alh ./
  exit 1
fi

